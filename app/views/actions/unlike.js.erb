<% if @result == "true" %>
	$("div#like_status").html("<%= escape_javascript(link_to(t('common.like'), action_like_path(:slug => @art.slug), :remote => true, :class => ['abutton', 'like'], :walk => 'no')) %>");
	$("p#liked_by").html("<%= escape_javascript(render :partial => 'partials/art/profile/liked_by') %>")
	flashBar("You stopped liking '<%= @art.name %>'.", "notice", {});
<% else %>
	alert("Error inesperado.");
<% end %>