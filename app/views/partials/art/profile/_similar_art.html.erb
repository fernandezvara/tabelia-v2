<div class="box green">
  <div class="title"><%= t('arts.profile.similar_art') %><%= link_to t('common.see_all_female'), user_arts_path(:username => @art.user.username)%></div>
  <div class="content">
    <% @similar_art.each do |art| %>
    <div class="image_thumb">
      <%= link_to image_tag(art.image.url(:thumb).to_s, :alt => art.name), art_profile_path(:slug => art.slug) %>
    </div>
    <% end %>
  </div>
</div>